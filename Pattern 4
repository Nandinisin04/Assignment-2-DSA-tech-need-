public class Pattern {
    public static void main (String args[]){
        //outer
        for (int i =1 ; i<=n ; i++){
            //Spaces
            for(int j=n-i; j<=1 ; j--){
                System.out.print(" ");
            }
            //Stars
            for (int j=i ; j<=1 ; j--){

            }
            System.out.println();
        }
    }
}
